{
  "name": "comic-generator",
  "version": "1.0.0",
  "description": "AI Image Generator using Minimax API",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && concurrently \"tsc --watch\" \"nodemon --watch dist --delay 2 dist/index.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "inworld",
    "ai",
    "comic",
    "image-generator"
  ],
  "author": "Inworld AI",
  "dependencies": {
    "@inworld/runtime": "^0.6.4",
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "uuid": "^10"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
